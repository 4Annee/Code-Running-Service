{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "spring": {
    "application": {
      "name": "CodeService"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8888/eureka",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "CodeService",
      "hostName": "localhost",
      "port": "5200"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CodeServiceContext": "Server=(localdb)\\MSSQLLocalDB;Database=CodeService;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}